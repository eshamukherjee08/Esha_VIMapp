<% if notice %>
  <p id='notice'><%= notice%></p>
<% end %>

<script>

  function search_num() {
     $.ajax({
       url : "<%= find_search_data_path %>",
       dataType : 'script',
       type : 'get',
       data : "roll_num="+$("#search").attr("value")
     });
   }
</script>

<h1>SEARCH PANEL</h1>:
<%= text_field_tag('search', 'Enter Roll Number') %>
<input type="button", value="SEARCH", onclick="search_num();" >

<div id="result"> WELCOME </div>

<%= link_to 'Back To Main', root_path %>
